<?xml version="1.0" encoding="utf-8"?>
<otrs_config version="1.0" init="Config">
    <ConfigItem Name="DynamicFieldsImport::DoOverride" Required="0" Valid="1">
        <Description Translatable="1">Override existing dynamic fields when a field with an existing name is imported.</Description>
        <Group>ImportExportDynamicFields</Group>
        <SubGroup>Core</SubGroup>
        <Setting>
            <Option SelectedID="0">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Output::FilterElementPost###OutputFilterImportExportDynamicFields" Required="0" Valid="1">
        <Description Translatable="1">Show Import/Export widget in Postmaster filter screen.</Description>
        <Group>ImportExportDynamicFields</Group>
        <SubGroup>OutputFilter</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::FilterElementPost::ImportExportDynamicFields</Item>
                <Item Key="Debug">0</Item>
                <Item Key="Templates">
                    <Hash>
                        <Item Key="AdminDynamicFields">1</Item>
                    </Hash>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AdminImportExportDynamicFields" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for postmaster filter import/export module.</Description>
        <Description Lang="de">Frontendmodul-Registration f√ºr das Import/Export-Modul (Postmaster filter).</Description>
        <Group>ImportExportDynamicFields</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>admin</Group>
                <Description>Import/Export Dynamic Fields</Description>
                <NavBarName>Admin</NavBarName>
                <Title>Import/Export Dynamic Fields</Title>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
</otrs_config>

